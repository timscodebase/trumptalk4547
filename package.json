    "@types/express": "^5.0.0",
    "@types/node": "^22.9.1",
    "@types/nodemailer": "^6.4.17",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.0",
